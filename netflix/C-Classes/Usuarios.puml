@startuml Gerenciar perfil

abstract class Usuario {
    - nome: String
    - idioma: String
    - classificacaoEtaria : Integer
    - inicioAutomatico = True
    - tipoUsuario : TipoUsuario
    - Usuario Usuario(String nome, String idioma, Integer classificacaoEtaria)
    + HashMap<Integer, Midia>  gerarConteudo()
    + void setNome (String nome)
    + void setIdioma (String idioma)
    + void setClassificacaoEtaria (Integer classificacaoEtaria)
    + void setInicioAutomatico (Bool inicioAutomatico)
    + void setTipoUsuario (TipoUsuario tipoUsuario)
    + Secao navegarParacontinuarAssistindo (SecaoContinuarAssistindo secao)
    + Secao navegarParaBombando (SecaoBombando secao)
    + Secao navegarParaMinhaLista (SecaoMinhaLista secao)
    + override HashMap<Integer, Midia>  gerarConteudo(TipoUsuario tipoUsuario)
}

Enum TipoUsuario {
    GERAL, INFANTIL
}

Usuario -- TipoUsuario

@enduml